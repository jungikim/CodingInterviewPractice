   "source": [
    "\n",
    "# n items 0 .. n-1 in its own group\n",
    "g = list(range(n))\n",
    "\n",
    "def find(x):\n",
    "    if x != g[x]:\n",
    "        g[x] = find(g[x])\n",
    "    return g[x]\n",
    "\n",
    "# same group\n",
    "if find(i) == find(j):\n",
    "    continue\n",
    "\n",
    "# assign i group to j group\n",
    "g[find(j)] = find(i)\n"
   ]
  }
